{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "@jsejcksn"
  ],
  "overrides": [
    {
      "files": [
        "src/index.ts"
      ],
      "env": {
        "browser": true
      }
    },
    {
      "files": [
        "src/index.cjs.ts",
        "src/index.mjs.ts"
      ],
      "env": {
        "node": true
      }
    }
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "root": true
}
